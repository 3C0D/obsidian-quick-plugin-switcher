{
	"name": "quick-plugin-switcher",
	"version": "8.1.3",
	"description": "Adds a ribbon icon to quickly switch plugins with filters (all, enabled, disabled, most switched)",
	"main": "main.js",
	"scripts": {
		"dev": "tsx esbuild.config.mts",
		"build": "tsc -noEmit -skipLibCheck && tsx esbuild.config.mts production",
		"start": "npm i && tsx scripts/start.mts",
		"startf": "node scripts/start.mts -f",
		"version": "tsx scripts/update-version.mts",
		"acp": "tsx scripts/acp.mts",
		"bacp": "tsx scripts/acp.mts -b",
		"release": "tsx scripts/release.mts",
		"test": "tsx scripts/test-plugin.mts"
	},
	"keywords": [],
	"author": "1C0D",
	"license": "MIT",
	"devDependencies": {
		"@popperjs/core": "^2.11.8",
		"@types/fs-extra": "^11.0.4",
		"@types/markdown-it": "^13.0.2",
		"@types/node": "^16.11.6",
		"@types/semver": "^7.5.6",
		"@types/slug": "^5.0.7",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"dedent": "^1.5.1",
		"dotenv": "^16.4.1",
		"esbuild": "0.17.3",
		"fs": "^0.0.1-security",
		"fs-extra": "^11.2.0",
		"json-stringify-pretty-compact": "^4.0.0",
		"monkey-around": "^2.3.0",
		"obsidian": "latest",
		"path": "^0.12.7",
		"readline": "^1.3.0",
		"semver": "^7.5.4",
		"slug": "^8.2.3",
		"tslib": "2.4.0",
		"tsx": "^4.7.0",
		"typescript": "4.7.4"
	}
}